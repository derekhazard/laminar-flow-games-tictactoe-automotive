<?xml version="1.0" encoding="utf-8"?>
<!--
    Launcher icon foreground — 3×3 tic-tac-toe grid with X marks on the
    main diagonal (top-left, centre, bottom-right).

    Sizing rationale:
      The AAOS launcher clips the adaptive icon to the safe-zone circle:
      radius 36 centred at (54,54) in the 108×108 viewport.
      For the board's four corners to just touch that circle:
        side = 36 × √2 ≈ 50.9 dp  →  board (29,29)→(79,79)
        corner distance from centre = 25√2 ≈ 35.4 dp  ✓ inside radius 36

    Grid (each cell ≈16–17 dp):
      columns/rows: 29  46  62  79
    Outer boundary: (29,29)→(79,79).
    Grid dividers at x=46, x=62 (vertical) and y=46, y=62 (horizontal).

    X marks (10% smaller than cell padding, strokeWidth 5):
      cell (0,0)  centre (37.5,37.5)  half-span 4.95  →  arms (32.5,32.5)↔(42.5,42.5)  (42.5,32.5)↔(32.5,42.5)
      cell (1,1)  centre (54,54)      half-span 4.5   →  arms (49.5,49.5)↔(58.5,58.5)  (58.5,49.5)↔(49.5,58.5)
      cell (2,2)  centre (70.5,70.5)  half-span 4.95  →  arms (65.5,65.5)↔(75.5,75.5)  (75.5,65.5)↔(65.5,75.5)
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Outer boundary square -->
    <path
        android:pathData="M29,29 L79,29 L79,79 L29,79 Z"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="#00000000" />

    <!-- Grid dividers — subtle border colour -->
    <path
        android:pathData="M46,29 L46,79"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M62,29 L62,79"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,46 L79,46"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,62 L79,62"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (0,0) top-left -->
    <path
        android:pathData="M32.5,32.5 L42.5,42.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M42.5,32.5 L32.5,42.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (1,1) centre -->
    <path
        android:pathData="M49.5,49.5 L58.5,58.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M58.5,49.5 L49.5,58.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (2,2) bottom-right -->
    <path
        android:pathData="M65.5,65.5 L75.5,75.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M75.5,65.5 L65.5,75.5"
        android:strokeColor="#0096C7"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

</vector>
