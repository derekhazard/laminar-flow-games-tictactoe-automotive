<?xml version="1.0" encoding="utf-8"?>
<!--
    Launcher icon foreground — LF monogram over a subtle 3×3 tic-tac-toe grid.

    Sizing rationale:
      The AAOS launcher clips the adaptive icon to the safe-zone circle:
      radius 36 centred at (54,54) in the 108×108 viewport.
      Board (29,29)→(79,79) fits inside the safe zone (corner distance ≈ 35.4 dp).

    Grid (subtle, behind letters):
      Bounding box: (29,29)→(79,79).  Dividers at x={46,62} and y={46,62}.
      Color: #30363D, strokeWidth 2 (thinner than letters to recede visually).

    LF monogram (scaled from logo_lf.xml 48×37 letter area into the 50×50 grid,
    with 2dp inset for clearance from grid edges):
      L — accent blue #00B4D8, strokeWidth 4, strokeLineCap round.
      F — light gray #E6EDF3, strokeWidth 4, strokeLineCap round.
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Outer boundary square -->
    <path
        android:pathData="M29,29 L79,29 L79,79 L29,79 Z"
        android:strokeColor="#30363D"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="#00000000" />

    <!-- Grid dividers — subtle, behind letters -->
    <path
        android:pathData="M46,29 L46,79"
        android:strokeColor="#30363D"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M62,29 L62,79"
        android:strokeColor="#30363D"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,46 L79,46"
        android:strokeColor="#30363D"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M29,62 L79,62"
        android:strokeColor="#30363D"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- L: vertical bar -->
    <path
        android:pathData="M31,36 L31,72"
        android:strokeColor="#00B4D8"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- L: horizontal base -->
    <path
        android:pathData="M31,72 L48,72"
        android:strokeColor="#00B4D8"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- F: vertical bar -->
    <path
        android:pathData="M56,36 L56,72"
        android:strokeColor="#E6EDF3"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- F: top horizontal -->
    <path
        android:pathData="M56,36 L77,36"
        android:strokeColor="#E6EDF3"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- F: middle horizontal -->
    <path
        android:pathData="M56,53 L73,53"
        android:strokeColor="#E6EDF3"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

</vector>
