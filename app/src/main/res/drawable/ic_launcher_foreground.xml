<?xml version="1.0" encoding="utf-8"?>
<!--
    Launcher icon foreground — 3×3 tic-tac-toe grid with X marks on the
    main diagonal (top-left, centre, bottom-right).

    Sizing rationale:
      The adaptive icon circle mask has radius 54 centred at (54,54) in the
      108×108 viewport. For the board's four corners to just touch the inside
      edge of that circle the board's half-diagonal must equal 54:
        side = 54 × √2 ≈ 76dp
      Board: (16,16) → (92,92) — corners are ~53.7dp from centre. ✓

    Grid (each cell ≈25–26dp):
      columns/rows: 16  41  67  92
    Grid dividers at x=41, x=67 (vertical) and y=41, y=67 (horizontal).

    X marks (5dp padding inside each cell):
      cell (0,0)  16–41 / 16–41  →  arms (21,21)↔(36,36)  (36,21)↔(21,36)
      cell (1,1)  41–67 / 41–67  →  arms (46,46)↔(62,62)  (62,46)↔(46,62)
      cell (2,2)  67–92 / 67–92  →  arms (72,72)↔(87,87)  (87,72)↔(72,87)
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Grid dividers — subtle border colour -->
    <path
        android:pathData="M41,16 L41,92"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M67,16 L67,92"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M16,41 L92,41"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M16,67 L92,67"
        android:strokeColor="#30363D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (0,0) top-left -->
    <path
        android:pathData="M21,21 L36,36"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M36,21 L21,36"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (1,1) centre -->
    <path
        android:pathData="M46,46 L62,62"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M62,46 L46,62"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

    <!-- X — cell (2,2) bottom-right -->
    <path
        android:pathData="M72,72 L87,87"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />
    <path
        android:pathData="M87,72 L72,87"
        android:strokeColor="#0096C7"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:fillColor="#00000000" />

</vector>
